{
	"help": {
		"alias": [ "halp", "h", "help" ],
		"endpoint": "#",
		"help": "help [command](optional)",
		"description": "To get help on all supported commands, or a specified command"
	},

	"gem": {
		"alias": ["gem", "g"],
		"endpoint": "https://rubygems.org/api/v1/gems/{gem}.json",
		"help": "gem [gem-name]",
		"description": "Fetches details of the specified Ruby gem"
	},

	"bingang": {
    "alias": ["e", "empty", "c", "class"],
    "endpoint": "#",
    "help": "e [day(string)][class(number)] [builing]",
		"description": "find empty builing"
	},

  "subway" : {
    "alias": ["s", "subway", "r", "route"],
    "endpoint": "#",
    "help": "s [source(string)][destination(string)]",
		"description": "find shortest route by subway"
  },

	"error": {
		"exclude": true
	}
}
